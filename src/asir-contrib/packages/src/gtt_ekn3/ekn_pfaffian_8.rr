/*
phi<J>  <-->  v_J
*/
def phitov(JJ,K,N){
  Basis = mBasis(K,N);
  Rank=length(Basis);
  V=matrix(1,Rank);
  PhiJ=ltov([ltov([1,0]), ltov(JJ)]);
  for(J=0; J<Rank; J++){
    V[0][J]=cInts(PhiJ,ltov([ ltov([1,1]), Basis[J] ])); 
  }
  V=V*invintMatrix_n([0,K+N+1],[0,K+N+1],K,N);
  for(J=0; J<Rank; J++){
    V[0][J]=red(V[0][J]); 
  }	
  return(V);
}
