def pfaffianPsi(K,N){
  Psi=0;
  CalJ=calJ0(K,N);
  for (I=0; I<length(CalJ); I++){
    Psi+=pfaffianMJ(CalJ[I],K,N)*dlogxJ(CalJ[I],K,N);
  }
  return(Psi);
}
