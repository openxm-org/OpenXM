def eulerxij(I,A,P){
  if ((I[0]==A[0]) && (I[1]==A[1]+1)){return(1);}
  else if ((I[0]==P) && (I[1]==1)){return(1);}
  else if ((I[0]==A[0]) && (I[1]==1)){return(-1);}
  else if ((I[0]==P) && (I[1]==A[1]+1)){return(-1);}
  else {return(0);}
}
