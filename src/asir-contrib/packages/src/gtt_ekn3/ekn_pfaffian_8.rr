def cInts2(As,Bs){
  if (type(getopt(arule))>0) {
    Rule=getopt(arule);
    Cs=pm(As,Bs);
    if (Cs[0]==0){return(0); } 
    else if (Cs[0]==2){
      In=0; Id=1;
      N=length(Cs[1]);
      for (J=0; J<N; J++){
	In+=base_replace(alpha(Cs[1][J]),Rule);
	Id*=base_replace(alpha(Cs[1][J]),Rule);
      }
      return(As[0][0]*Bs[0][0]*In/Id);
    }else{
      Id=1;
      N=length(Cs[1]);
      for (J=0; J<N; J++){
	Id*=base_replace(alpha(Cs[1][J]),Rule);
      }
      return(Cs[0]/Id);
    }
  }else{return(cInts(As,Bs));}
}

