def prime_XRule(K,N){
  Rule=[];
  P=1;
  for (I=1; I<K+1; I++){
    for (J=1; J<N+1; J++){
      P=pari(nextprime,P+1);
      Rule=cons([xij(I,J),1/P],Rule);
    }
  }
  return(Rule);
}
