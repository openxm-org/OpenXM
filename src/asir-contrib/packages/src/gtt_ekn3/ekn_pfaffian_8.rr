/* initial of beta=[[1,1,...,(Total sum)-r/1+1],[beta^(2)]] */
def initialBeta_3(Beta){
  R1=length(Beta[0]);
  Brow=[base_sum(Beta[0],0,0,R1-1)-R1+1];
  for (J=1; J<R1; J++){
    Brow=cons(1,Brow);
  }
  return([Brow,Beta[1]]);
}

/* initial of alpha */
/* to make from beta --> marginaltoAlpha(initialBeta\_3(...)) */
def initialAlpha_3(A,K,N){
  IA=[];
  A0=0;
  for (J=N+1; J>0; J--){
    IA=cons(A[K+J],IA);
    A0+=A[K+J];
  }
  for (I=1; I<K+1; I++){
    IA=cons(-1,IA);
  }
  return(cons(K-A0,IA));
}

def initial_row(Q){
  if (Q<1){error("Q should be a positive integer.");}
  else if (Q==1){return([[1]]);}
  else {
    Rows=[];
    L0=[];
    Row1=initial_row(Q-1);
    for (I=1;I<Q;I++){L0=cons(0,L0);}
    L0=cons(1,L0);
    for (C=Row1; C!=[]; C=cdr(C)){
      Rows=cons(cons(0,car(C)),Rows);
    }
    return(cons(L0,Rows));
  }
}

def initialPoly_table_3(Beta){
  P=length(Beta[0]);
  Q=length(Beta[1]);
  if (P < 2) error("We assume #(rows) >= 2.");
  
  Ini=initial_row(Q);
  T=[];
  if (P==2){
    RQ=matrix_list_to_matrix(Beta[1]);
    for (C=Ini; C!=[]; C=cdr(C)){
      L1=car(C);
      L2=matrix_matrix_to_list(RQ-matrix_list_to_matrix(L1));
      T=cons([L1,L2],T);
    }
    return(T);
  }else{
    T1=initialPoly_table_3([cdr(Beta[0]),Beta[1]]);
    for (TT=T1; TT !=[]; TT=cdr(TT)){
      U=reverse(car(TT));
      for (C=Ini; C!=[]; C=cdr(C)){
	L1=car(C);
	L2=matrix_matrix_to_list(matrix_list_to_matrix(car(U))-matrix_list_to_matrix(L1));
	Positive=0;
	L3=L2;
	while((L3 != []) && (Positive==0)){
	  if (car(L3)<0) Positive++; 
	  L3=cdr(L3);
	}
	if (Positive==0) T=cons(cons(L1,reverse(cons(L2,cdr(U)))),T);
      }
    }
    return(T);
  }
}

def initialPoly_3(Beta){
  P=length(Beta[0]); 
  Q=length(Beta[1]); 
  TL=initialPoly_table_3(Beta);
  S=0;
  for (B=TL; B != []; B=cdr(B)){
    B0=car(B);
    T=1/factorial(B0[P-1][0]);
    for (J=1; J<Q; J++){
      for (I=0; I<P-1 ;I++){
	if (B0[I][J] == 1) T*=xij(I+1,J);
      }
      T*=1/factorial(B0[P-1][J]);
    }
    S+=T;
  }
  return(S);
}

