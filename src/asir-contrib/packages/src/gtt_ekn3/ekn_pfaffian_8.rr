}
def contiguity_mat_list_3_dist(A,K,N){
  Idl = get_ekn_IDL();
  // if (XRule != 0){ XR=XRule; }
  // else{ XR=[]; }
  if (Idl == 0) error("Error. Start subprocesses by setup(N| ...prog=...");
  IDList=Idl;
  if (type(getopt(xrule))>0) {
    XR=getopt(xrule);
  }else{XR=[];}
  Al=initialAlpha_3(A,K,N);
  Alv=ltov(Al);
  Av=ltov(A);
  Step=Av-Alv;

  CList=newvect(K+1); Ekn_CList=newvect(K+1);
  /* J=1,...,K */
  List=[]; /* process numbers */
  for (J=1,II=0; J<K+1 && II<length(IDList); J++,II++){
    if (Step[J]<0){
      Amove=moveAlpha(J,vtol(Alv),K,N); 
      // C=base_replace(downAlpha(J,K,N),alphaRule_num(Amove,K,N));
      ox_rpc(IDList[II],"gtt_ekn3.downAlpha3_ax",J,K,N,alphaRule_num(Amove,K,N),XR);
      ox_push_cmd(IDList[II],258);
      List = cons(IDList[II],List);
      CList[J]=[C,Step[J]];  /* C has not been obtained */
      Ekn_CList[J]=[C,Step[J],J];
      Alv[0]=Alv[0]-Step[J];
      Alv[J]=Av[J];
    }else{}
    // printf("%a\n",Alv);
  }

  while(List != []){
    E = ox_select(List);
    while(E != []){
      T = ox_get(car(E));
      C=T[0]; HH=CList[T[1]]; CList[T[1]]=[C,HH[1]];
      HH=Ekn_CList[T[1]]; Ekn_CList[T[1]]=[C,HH[1],HH[2]];
      if(J < K+1){
       if (Step[J]<0) { 
        Amove=moveAlpha(J,vtol(Alv),K,N); 
        ox_rpc(car(E),"gtt_ekn3.downAlpha3_ax",J,K,N,alphaRule_num(Amove,K,N),XR);
        II=base_position(car(E),IDList); 
        List = cons(IDList[II],List);
        CList[J]=[C,Step[J]];  /* C has not been obtained */
        Ekn_CList[J]=[C,Step[J],J];
        Alv[0]=Alv[0]-Step[J];
        Alv[J]=Av[J];
        ox_push_cmd(car(E),258);
        J++;
       }else { ; }
      }else{
        List = rm_list(List,car(E));
      }
      E = cdr(E);
    }
  }
  Ekn_CList = base_prune(0,vtol(Ekn_CList));
  CList=base_prune(0,vtol(CList));
  // printf("%a=CList\n",vtol(CList)); // 2021.08.02
  return(cons(t,CList));
}


