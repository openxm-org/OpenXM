def moveAlpha_n(I,A,K,N){
  Amove1=[];
  Amove2=[];
  for (J=K+N+1; J>0; J--){
    if (J==I){ Amove1=cons([alpha(J),A[J]+t],Amove1); }
    else{ Amove2=cons([alpha(J),A[J]],Amove2);}
  }
  Amove1=cons([alpha(0),A[0]-t],Amove1);
  return([Amove1,Amove2]);
}
