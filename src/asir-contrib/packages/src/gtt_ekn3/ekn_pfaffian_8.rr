/*
intersection numbers for type [[sign, checker],A]
*/
def cInts(As,Bs){
  Cs=pm(As,Bs);
  if (Cs[0]==0){return(0); } 
  else if (Cs[0]==2){
    In=0; Id=1;
    N=length(Cs[1]);
    for (J=0; J<N; J++){
      In+=alpha(Cs[1][J]);
      Id*=alpha(Cs[1][J]);
    }
    return(As[0][0]*Bs[0][0]*In/Id);
  }else{
    Id=1;
    N=length(Cs[1]);
    for (J=0; J<N; J++){
      Id*=alpha(Cs[1][J]);
    }
    return(Cs[0]/Id);
  }
}
