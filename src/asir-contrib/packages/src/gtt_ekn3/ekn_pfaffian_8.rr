/* max of a list */
def max(A){
  N=length(A);
  Max=A[0];
  for (K=1; K<N; K++) {
    if (Max < A[K]) {
      Max = A[K];
    }
  }
  return(Max);
}

/* factorial */
def factorial(N) {
  if (N < 0) error("factorial: argument must be 0 or natural numbers.");
  if (N == 0) return(1);
  else {
    return(N*factorial(N-1));
  }
}

def factorial_prod(B){
  N=1;
  for (C=B; C!=[]; C=cdr(C)){
    N*=factorial(C[0]);
  }
  return(N);
}
