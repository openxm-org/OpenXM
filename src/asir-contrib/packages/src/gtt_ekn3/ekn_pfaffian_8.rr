    for ( P = 0; P < length(JJ); P++ ){ Prod*=alpha(JJ[P]); }
    MJ=base_replace(Prod*intMatrixH(K,N)*matrix_transpose(VJ)*VJ,alphaRule(K,N));
    if(ARule != 0){ return(base_replace(MJ,ARule)); }
    else{ return(MJ); }
  }else{
    return(0);
  }
}
