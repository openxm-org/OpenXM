  Rep=base_replace(repMatrix(II,K,N),Rule);
  Rule0=[[alpha(0),alpha(0)-1],[alpha(II),alpha(II)+1]];
  T1 = base_replace(intMatrix([0,K+N+1],[0,K+N+1],K,N),Rule0);
  T1 = base_replace(T1,Rule);
  T2 = base_replace(invintMatrix_n([II,0],[0,K+N+1],K,N),Rule0);
  T2 = base_replace(T2,Rule);
  Pinv=T1*T2;
  Q=base_replace(intMatrix([0,II],[0,K+N+1],K,N),Rule)
    *base_replace(invintMatrix_n([0,K+N+1],[0,K+N+1],K,N),Rule); 
  return(base_replace(coef_upAlpha(II,K,N),Rule)*Pinv*Rep*Q);
}
/*
  RuleA=[[a_2,1/3],[a_3,1/2]]$ RuleX=[[x_1_1,1/5]]$
  base_replace(gtt_ekn.upAlpha(1,1,1),append(RuleA,RuleX))-gtt_ekn.upAlpha(1,1,1 | arule=RuleA, xrule=RuleX);
  base_replace(gtt_ekn.downAlpha(1,1,1),append(RuleA,RuleX))-gtt_ekn.downAlpha(1,1,1 | arule=RuleA, xrule=RuleX);
*/

def downAlpha(II,K,N){
  if ((type(getopt(arule))>0) || (type(getopt(xrule))>0)) {
     return downAlpha2(II,K,N | option_list=getopt());
  }
  Rep=invrepMatrix(II,K,N);
  Qinv=base_replace(intMatrix([0,K+N+1],[0,K+N+1],K,N)*invintMatrix_n([0,II],[0,K+N+1],K,N), 
		 [[alpha(0),alpha(0)+1],[alpha(II),alpha(II)-1]]);
  P=intMatrix([II,0],[0,K+N+1],K,N)*invintMatrix_n([0,K+N+1],[0,K+N+1],K,N); 
  Coef=base_replace(coef_upAlpha(II,K,N),[[alpha(0),alpha(0)+1],[alpha(II),alpha(II)-1]]);
  return((1/Coef)*Qinv*Rep*P);
}
// I=1$ C1=upAlpha(I,2,2)$ C2=base_replace(downAlpha(I,2,2),[[alpha(0),alpha(0)-1],[alpha(I),alpha(I)+1]])$ C3=C2*C1$ mat_red(C3)$ base_replace(C3,alphaRule(2,2));

def downAlpha2(II,K,N){
  Rule=[];
  if (type(getopt(arule))>0) {
    Rule=append(Rule,getopt(arule));
  }
  if (type(getopt(xrule))>0) {
    Rule=append(Rule,getopt(xrule));
  }
  Rep=base_replace(invrepMatrix(II,K,N),Rule);
  Rule0=[[alpha(0),alpha(0)+1],[alpha(II),alpha(II)-1]];
  T1 = base_replace(intMatrix([0,K+N+1],[0,K+N+1],K,N),Rule0);
  T1 = base_replace(T1,Rule);
  T2 = base_replace(invintMatrix_n([0,II],[0,K+N+1],K,N),Rule0);
  T2 = base_replace(T2,Rule); 
  Qinv=T1*T2;
  P=base_replace(intMatrix([II,0],[0,K+N+1],K,N),Rule)
   *base_replace(invintMatrix_n([0,K+N+1],[0,K+N+1],K,N),Rule); 
  Coef=base_replace(coef_upAlpha(II,K,N),[[alpha(0),alpha(0)+1],[alpha(II),alpha(II)-1]]);
  Coef=base_replace(Coef,Rule);
  return((1/Coef)*Qinv*Rep*P);
}

/*
  function F
  -> vector [F, diff(F,x_11), ...] <- column vector
*/
def pfaffian_basis(F,K,N){
  B=mBasisList(K,N);
  Rank=length(B);
  SS=matrix(Rank,1);
  SS[0][0]=F;
  for (I=1; I<Rank; I++){
    C1=[]; C2=[];
    for (J=1; J<K+1; J++){
      if (!base_memberq(J,B[I])){C1=cons(J,C1);}
    }
    for (J=K+1; J<K+N+1; J++){
      if (base_memberq(J,B[I])){C2=cons(J,C2);}
    } 
    SS[I][0]=SS[0][0];
    for (J=0; J<length(C1); J++){
      SS[I][0]=diff(SS[I][0],xij(C1[J],C2[J]-K))/alpha(C2[J]);
    }
    SS[I][0]=SS[I][0]*matrix_det(xJ(B[I],N));
  }
  return(SS);
}

