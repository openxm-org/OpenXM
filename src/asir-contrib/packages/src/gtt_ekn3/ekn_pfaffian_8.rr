    for (B=C; B != []; B=cdr(B)){
      B0=car(B);
      T=1/(factorial(length(B0[1])));
      for (A=B0[0]; A != []; A=cdr(A)){
	A0=car(A);
	T*=xij(A0[1],A0[0]);
      }
      S+=T;
    }
  }else{
    C=initialPoly_table(K-1,N);
    for (B=C; B != []; B=cdr(B)){
      B0=car(B);
      T=1/(factorial(length(B0[1])));
      for (A=B0[0]; A != []; A=cdr(A)){
	A0=car(A);
	T*=xij(A0[0],A0[1]);
      }
      S+=T;
    }
  }
  return(S);
}
