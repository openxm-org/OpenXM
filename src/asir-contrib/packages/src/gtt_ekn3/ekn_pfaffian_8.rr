/* Laurent -> poly */
def ltop(F,Beta,X,K,N){
  P=1;
  for (J=0; J<K; J++){
    P*=X[J][0]^Beta[0][J];
  }
  for (J=0; J<N; J++){
    P*=X[K][J+1]^Beta[1][J+1];
  }
  Sa=Beta[1][0];
  for (J=0; J<K; J++){
    Sa-=Beta[0][J];
  }
  P*=X[K][0]^Sa;
  return(F*P);
}
