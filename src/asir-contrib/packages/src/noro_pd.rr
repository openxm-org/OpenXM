  G = nd_gr_trace(B,V1,1,Check,[[0,length(W)],[0,length(PV1)]]|nora=1);
  G = elimination(G,PV1);
  G = nd_gr_trace(G,PV1,1,Check,[[0,1],[0,length(PV)]]|nora=1);
  for ( M = car(G), T = cdr(G); T != []; T = cdr(T) )
    if ( deg(car(T),VI) < deg(M,VI) ) M = car(T);
  return sdiv(M,cont(M,VI));
}

