  } else if ( Competitive && Procs ) {
    Arg0 = ["nd_f4",A,V,0,Ord];
    Arg1 = ["nd_f4_trace",A,V,1,GBCheck,Ord];
    G = competitive_exec("incremental_gb",Procs,[Arg0,Arg1]);
  } else
    G = fast_gb(A,V,0,Ord|gbblock=Block);
  return G;
}

