def sat_ind_var(G,F,V)
{
  if ( type(Ord=getopt(ord)) == -1 ) Ord = 0;
  if ( type(Mod=getopt(mod)) == -1 ) Mod = 0;
  V0 = append(setminus(V,[F]),[F]);
  G0 = nd_gr(G,V0,Mod,0);
  M = 0;
  for ( G1 = [], T = G0; T != []; T = cdr(T) ) {
  S = car(T);
  M1 = mindeg(S,F);
  S = sdiv(S,F^M1);
  G1 = cons(S,G1);
  if ( M1 > M ) M = M1;
  }
  G1 = nd_gr(G1,V,Mod,Ord);
