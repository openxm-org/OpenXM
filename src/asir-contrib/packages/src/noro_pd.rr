  G = nd_gr_postproc(B,V,0,0,0);
  V = vars(G);
  N = length(V);
  if ( N == 0 ) return G == [] ? [[]] : [];
  if ( N == 1 ) return G;
  if ( N < 20 ) {
    T = dp_mono_raddec(G,V);
    return map(prod,T);
  }
  X = car(V); W = cdr(V);
  D0 = monodec(map(subst,B,X,0),W);
  T0 = map(dp_ptod,D0,W);
  D1 = monodec(map(subst,B,X,1),W);
  T1 = map(dp_ptod,D1,W);
#if 0
