    }
	if((E=getopt(e))==1||E==2){
	  if(length(N)==4) N=cdr(N);
	  if(length(N)==3) return vConv(N[0],N[1],N[2]|var=X,e=E);
	}
	for(M=[],I=S=0;I<K;Z=0,I++){
		M=cons(mcvm(N[I]|var=X[I],z=Z),M);
