			return mydeval(mysubst(F,[X[0],X[1]]));
		if(type(X[0])==4&&isvar(X[0][0])&&length(X[0])==2){
			for(Y=X;Y!=[];Y=cdr(Y))
				F=mysubst(F,[car(Y)[0],car(Y)[1]]);
			return myeval(F);
		}
	}
	return myeval(mysubst(F,[x,X]));
}

