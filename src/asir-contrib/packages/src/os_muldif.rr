					if(F==2) SS=cdr(SS);
					for(J=0;J<12;J++) SS=cdr(SS);
				}
				continue;
			}
			S=cons(car(SS),S);
		}
		SS=reverse(S);
		Top=P;
	}
    for(F=G=0,S=[];SS!=[];SS=cdr(SS)){  /* 22^x -> 2\cdot 2^x */
		if(F==1&&G!=-1&&car(SS)==123 && length(SS)>1 && isnum(SS[1]))
			S=append([116,111,100,99,92],S);
		G=F;
		if(car(SS)==125||car(SS)==95) F=-1;
		else F=isnum(car(SS));
		S=cons(car(SS),S);
