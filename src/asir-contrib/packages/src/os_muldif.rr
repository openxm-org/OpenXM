		else if(G==0) R=FT[2];
#ifdef USEMODULE
			else R=eval(call(G[0],map(os_md.myeval,cdr(cdr(FT)))|option_list=G[1]));
#else
			else R=eval(call(G[0],map(myeval,cdr(cdr(FT)))|option_list=G[1]));
