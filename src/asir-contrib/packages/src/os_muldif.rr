					S=cons(123,S);
					if(F==2) SS=cdr(SS);
					else if(F==0) S=cons(car(SS),S);
				}else if(F==2&&P-Q==3){		/* (2)^x -> 2^x */
					SS=cdr(SS);SS=cdr(SS);
					S=cons(123,S);S=cons(car(SS),S);S=cons(125,S);
					SS=cdr(SS);SS=cdr(SS);
					I+=3;
