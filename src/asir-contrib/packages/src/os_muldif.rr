					if(J-- <= 0 || S[J] < 65 || S[J] > 122 || (S[J] > 90 && S[J] < 97)){
						SS = cons(S[I],SS);
						L = J+1;
						continue;
					}
				}
			}
		}
		SS = cons(S[L],SS);
	}
	RT=getopt(root);
	for(Top=0;;Top++){	/* ((x+1))^{y} , 1/y=2,3,...,9 */
#if 1
		P=str_str(SS,["))^","^{\\tfrac{1}"]|top=Top);
		if(P[0]<0) break;
		Sq=0;
		if(P[0]==0){
