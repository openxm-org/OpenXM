				str_tb(xyarrow(T1,T2|option_list=Opt),Out);
			}else if(T[0]==4){
				F++;
				T1=T[2];T2=T[3];
				if(M){
					T1=ptaffine(M,T1|option_list=Org);
					T2=ptaffine(M,T2|option_list=Org);
				}
				str_tb(xyline(T1,T2|option_list=Opt),Out);
			}else if(T[0]==5){	
				if(F){
					S=str_tb(0,Out);
					Out=str_tb(0,0);
					F=0;
					if(S!=""){
						if(P[0]==2) dviout(xyproc(S)|keep=1);
